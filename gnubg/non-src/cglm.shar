X  float fn, farVal, nearVal, p22, p32;
X
X  p22        = proj[2][2];
X  p32        = proj[3][2];
X
X  nearVal    = p32 / (p22 - 1.0f);
X  farVal     = p32 / (p22 + 1.0f) + deltaFar;
X  fn         = 1.0f / (nearVal - farVal);
X
X  proj[2][2] = (nearVal + farVal) * fn;
X  proj[3][2] = 2.0f * nearVal * farVal * fn;
X}
