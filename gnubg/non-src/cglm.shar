X  CGLM_ALIGN(8) vec3 axis;
X  float dot, angle;
X
X  dot = glm_vec3_dot(dir, fwd);
X  if (fabsf(dot + 1.0f)  < 0.000001f) {
X    glm_quat_init(dest, up[0], up[1], up[2], GLM_PIf);
X    return;
X  }
X
X  if (fabsf(dot - 1.0f) < 0.000001f) {
X    glm_quat_identity(dest);
X    return;
X  }
X
X  angle = acosf(dot);
X  glm_cross(fwd, dir, axis);
X  glm_normalize(axis);
