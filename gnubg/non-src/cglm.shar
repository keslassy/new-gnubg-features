X/* DEPRECATED! use _copy, _ucopy versions */
X#define glm_mat4_udup(mat, dest) glm_mat4_ucopy(mat, dest)
X#define glm_mat4_dup(mat, dest)  glm_mat4_copy(mat, dest)
