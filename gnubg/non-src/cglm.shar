X */
XCGLM_INLINE
Xfloat
Xglm_hermite(float s, float p0, float t0, float t1, float p1) {
X  float ss, d, a, b, c, e, f;
X
X  ss = s  * s;
X  a  = ss + ss;
X  c  = a  + ss;
X  b  = a  * s;
X  d  = s  * ss;
X  f  = d  - ss;
X  e  = b  - c;
X
X  return p0 * (e + 1.0f) + t0 * (f - ss + s) + t1 * f - p1 * e;
X}
