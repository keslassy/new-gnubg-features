X   dest[3][0] =-(e * t1[2] - f * t1[4] + g * t1[5]);
X   dest[3][1] =  a * t1[2] - b * t1[4] + c * t1[5];
X   dest[3][2] =-(a * t2[2] - b * t2[4] + c * t2[5]);
X   dest[3][3] =  a * t3[2] - b * t3[4] + c * t3[5]; */
X  v3 = _mm_add_ps(_mm_mul_ps(x2, t5),
X                  _mm_sub_ps(_mm_mul_ps(x0, t2),
X                             _mm_mul_ps(x1, t4)));
