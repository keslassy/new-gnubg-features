X  glmm_store(dest[0], _mm_mul_ps(v0, x0));
X  glmm_store(dest[1], _mm_mul_ps(v1, x0));
X  glmm_store(dest[2], _mm_mul_ps(v2, x0));
X  glmm_store(dest[3], _mm_mul_ps(v3, x0));
X}
